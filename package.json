{
  "name": "ronvandekerkhof.nl",
  "version": "1.0.0",
  "description": "",
  "main": "lib/server/index.js",
  "scripts": {
    "start": "concurrently 'npm run server' 'npm run client'",
    "start:watch": "concurrently 'npm run server:watch' 'npm run client:watch'",
    "server": "babel-node src/server/index.js",
    "server:watch": "nodemon -e 'js jsx' --ignore 'public/dist/**/*.js' --ignore 'src/client/**/*.js' --exec 'npm run start'",
    "compile": "npm run client && babel src --out-dir lib",
    "postinstall": "npm install --only=dev --ignore-scripts && npm run compile",
    "eslint": "eslint src",
    "lint": "npm run eslint",
    "lint:watch": "nodemon --exec 'npm run lint'",
    "client": "webpack",
    "client:watch": "nodemon -e 'js jsx' --ignore 'public/dist/**/*.js' --ignore 'src/server/**/*.js' --exec 'npm run client'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PepijnSenders/ronvandekerkhof.nl.git"
  },
  "keywords": [],
  "author": "Pepijn Senders <ps@hellofresh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PepijnSenders/ronvandekerkhof.nl/issues"
  },
  "homepage": "https://github.com/PepijnSenders/ronvandekerkhof.nl#readme",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "body-parser": "^1.15.2",
    "chroma-js": "^1.2.1",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphiql": "^0.7.4",
    "graphql": "^0.6.2",
    "graphql-date": "^1.0.2",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.8",
    "mongoose-timestamp": "^0.6.0",
    "normalize.css": "^4.2.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.7",
    "redux-logger": "^2.6.1",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-html-template": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-senf": "git+https://git@github.com/hellofresh/babel-preset-senf.git",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "eslint-config-senf": "git+https://git@github.com/hellofresh/eslint-config-senf.git",
    "nodemon": "^1.9.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "engines": {
    "node": ">= 6.1.0"
  }
}
