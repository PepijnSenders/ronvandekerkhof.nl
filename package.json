{
  "name": "ronvandekerkhof.nl",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run server' 'npm run client'",
    "start:watch": "concurrently 'npm run server:watch' 'npm run client:watch'",
    "server": "babel-node src/server/index.js",
    "server:watch": "nodemon -e 'js jsx' --ignore 'public/dist/**/*.{js,jsx}' --ignore 'src/client/**/*.{js,jsx}' --exec 'npm run start'",
    "compile": "babel src/ --out-dir lib",
    "postinstall": "npm run compile",
    "eslint": "eslint './src/**/*.{js,jsx}'",
    "lint": "npm run eslint",
    "lint:watch": "nodemon --exec 'npm run lint'",
    "client": "webpack",
    "client:watch": "nodemon -e 'js jsx' --ignore 'public/dist/**/*.{js,jsx}' --ignore 'src/server/**/*.{js,jsx}' --exec 'npm run client'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PepijnSenders/ronvandekerkhof.nl.git"
  },
  "keywords": [],
  "author": "Pepijn Senders <ps@hellofresh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PepijnSenders/ronvandekerkhof.nl/issues"
  },
  "homepage": "https://github.com/PepijnSenders/ronvandekerkhof.nl#readme",
  "dependencies": {
    "body-parser": "^1.15.1",
    "chroma-js": "^1.1.1",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "method-override": "^2.3.6",
    "pug": "^2.0.0-beta2",
    "radium": "^0.17.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-logger": "^2.6.1",
    "redux-router": "^1.0.0-beta8",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.5.0",
    "concurrently": "^2.1.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.1"
  }
}
